  </ItemGroup>

  <ItemGroup>
    <Content Include="Elibre.ico" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Elibre.jpg" />
    <Resource Include="Elibre.png" />
  </ItemGroup>

</Project>
