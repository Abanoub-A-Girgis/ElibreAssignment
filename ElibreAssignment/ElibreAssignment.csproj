    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>Elibre.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Elibre.jpg" />
